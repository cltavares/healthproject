<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AlertActivity"
            android:label="Alertas"/>
        <activity android:name=".OnlineAppointmentActivity"
            android:label="Consulta Online"/>
        <activity android:name=".PhysicalHealthActivity"
            android:label="Saúde Física"/>
        <activity android:name=".EmotionalHealthActivity"
            android:label="Saúde Emocional"/>
        <activity android:name=".RealTimeHealthActivity"
            android:label="Nota geral"/>
        <activity android:name=".HomeActivity" />
        <activity android:name=".SignUpActivity"
            android:label="Cadastro"/>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>